local Footer = {}

function Footer.Main(props)
  return (
    <footer>
      {props.children}
    </footer>
  )
end

function Footer.Container(props)
  return (
    <div>
      {props.children}
    </div>
  )
end

function Footer.Text(props)
  return (
    <p>
      {props.children}
    </p>
  )
end

return Footer