local Hero = {}

function Hero.Main(props) 
  return (
    <section>
      {props.children}
    </section>
  )
end

function Hero.Title(props) 
  return (
    <h1>
      {props.children}
    </h1>
  )
end

function Hero.Subtitle(props) 
  return (
    <p>
      {props.children}
    </p>
  )
end

function Hero.CTAButton(props) 
  return (
    <button>
      {props.children}
    </button>
  )
end

return Hero